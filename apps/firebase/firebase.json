{
  "firestore": {
    "database": "(default)",
    "location": "asia-northeast1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "hosting_contents",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "auth": {
      "port": 9199,
      "host": "127.0.0.1"
    },
    "firestore": {
      "port": 8180,
      "host": "127.0.0.1"
    },
    "hosting": {
      "port": 5100,
      "host": "127.0.0.1"
    },
    "ui": {
      "enabled": true,
      "port": 4100,
      "host": "127.0.0.1"
    },
    "singleProjectMode": true,
    "hub": {
      "port": 4600,
      "host": "127.0.0.1"
    }
  }
}
